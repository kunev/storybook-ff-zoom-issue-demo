{
  "semi": false,
  "singleQuote": true,
  "bracketSpacing": false,
  "printWidth": 120,
  "endOfLine": "lf",
  "arrowParens": "avoid",
  "trailingComma": "none",
  "htmlWhitespaceSensitivity": "ignore",

  "overrides": [
    {
      "files": "*.ts*",
      "options": {
        "parser": "typescript"
      }
    }
  ]
}
